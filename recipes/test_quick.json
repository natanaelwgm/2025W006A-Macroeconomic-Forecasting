{
  "run_name": "test_quick",
  "targets": [
    {"id": "cpi_yoy", "name": "CPI YoY", "horizons": [1, 3]}
  ],
  "frequency": "M",
  "strategy": "frozen",
  "train": { "start": "2010-01-31", "end": "2019-12-31" },
  "test": { "start": "2020-01-31", "end": "2023-12-31" },
  "data": {
    "path": "data/processed/indonesia_macro_monthly.csv",
    "date_col": "date"
  },
  "models_filter": ["AR1", "RandomForest"],
  "features": {
    "target_lags": [1, 3],
    "exog": { "__all__": { "lags": [0] } },
    "sweep": {
      "packs": ["none"],
      "normalize": [{"method": "none"}],
      "max_features": [30],
      "exog_combo_k": 2,
      "exog_combo_limit": 2
    }
  },
  "output": { "dir": "outputs" }
}
