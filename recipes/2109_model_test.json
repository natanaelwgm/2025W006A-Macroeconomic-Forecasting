{
  "run_name": "2109_model_test",
  "targets": [
    {
      "id": "cpi_yoy",
      "name": "CPI Year-over-Year", 
      "horizons": [1, 3, 6]
    },
    {
      "id": "gdp_yoy", 
      "name": "GDP Year-over-Year",
      "horizons": [1, 3, 6]
    },
    {
      "id": "usd_idr",
      "name": "USD/IDR Exchange Rate",
      "horizons": [1, 3, 6] 
    },
    {
      "id": "policy_rate_7drr",
      "name": "BI 7-Day Reverse Repo Rate",
      "horizons": [1, 3, 6]
    }
  ],
  "frequency": "M",
  "strategy": "frozen",
  "train": { "start": "2010-01-31", "end": "2019-12-31" },
  "test": { "start": "2020-01-31", "end": "2024-12-31" },
  "data": {
    "path": "data/processed/indonesia_macro_monthly.csv",
    "date_col": "date"
  },
  "models_filter": [
    "DFM",
    "DFM2", 
    "RandomForest",
    "ElasticNet",
    "Lasso",
    "Ridge",
    "AR1"
  ],
  "features": {
    "target_lags": [1, 3, 6],
    "exog": { "__all__": { "lags": [0, 1, 3] } },
    "sweep": {
      "packs": ["none"],
      "normalize": [
        {"method": "none"},
        {"method": "standardize"}
      ],
      "max_features": [25, 40],
      "exog_combo_k": 3,
      "exog_combo_limit": 4
    }
  },
  "output": { "dir": "outputs" }
}
