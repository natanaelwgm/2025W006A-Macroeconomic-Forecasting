{
  "base": {
    "target_id": "y",
    "frequency": "M",
    "horizons": [1, 2, 3],
    "strategy": "frozen",
    "as_of_date": "2025-08-31",
    "train": { "start": "2010-01-31", "end": "2019-12-31" },
    "test": { "start": "2020-01-31", "end": "2025-07-31" },
    "data": {
      "path": "v2/data/synthetic/synthetic_monthly.csv",
      "date_col": "date"
    },
    "features": { "target_lags": [0], "exog": {} },
    "output": { "dir": "v2/outputs" }
  },
  "runs": [
    { "model": { "name": "Naive" }, "features": { "target_lags": [0] } },
    { "model": { "name": "AR1" },   "features": { "target_lags": [0] } },
    { "model": { "name": "ARp" },   "features": { "target_lags": [1,2,3] } },
    { "model": { "name": "SeasonalNaive", "params": { "period": 12 } }, "features": { "target_lags": [12] } },
    { "model": { "name": "Linear" }, "features": { "target_lags": [0], "exog": { "x1": { "lags": [0,1] }, "x2": { "lags": [0,1] } } } },
    { "model": { "name": "Ridge", "params": { "alpha": 1.0 } }, "features": { "target_lags": [0], "exog": { "x1": { "lags": [0,1] }, "x2": { "lags": [0,1] } } } },
    { "model": { "name": "Ridge", "params": { "alpha": 10.0 } }, "features": { "target_lags": [0], "exog": { "x1": { "lags": [0,1] }, "x2": { "lags": [0,1] } } } }
  ]
}

