{
  "run_name": "enhanced_comprehensive_all_models_indonesia_macro_monthly",
  "targets": [
    {
      "id": "CPI",
      "name": "CPI",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "Real GDP Growth",
      "name": "Real GDP Growth",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "BI7DRR",
      "name": "BI7DRR",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "Deposit Rate 1M",
      "name": "Deposit Rate 1M",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "Deposit Rate 3M",
      "name": "Deposit Rate 3M",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "Deposit Rate 6M",
      "name": "Deposit Rate 6M",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "Deposit Rate 12M",
      "name": "Deposit Rate 12M",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "Govt Bond Yield 10 Yr",
      "name": "Govt Bond Yield 10 Yr",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "JISDOR Exchange Rate",
      "name": "JISDOR Exchange Rate",
      "horizons": [1, 3, 6, 12, 24]
    },
    {
      "id": "FX",
      "name": "USD/IDR Exchange Rate",
      "horizons": [1, 3, 6, 12, 24]
    }
  ],
  "frequency": "M",
  "strategy": "frozen",
  "train": { "start": "2005-01-31", "end": "2019-12-31" },
  "test": { "start": "2020-01-31", "end": "2025-07-31" },
  "data": {
    "path": "data/processed/indonesia_macro_monthly.csv",
    "date_col": "date"
  },
  "models_filter": [
    "ARIMA", "MIDAS", "DNS", "TVP", "NeuroGARCH",
    "Bagging", "ElasticNetGrid", "Huber", "Naive", "PLS1",
    "SeasonalNaive", "StandardizedLinear", "StandardizedRidge",
    "AR1", "ARp", "Linear", "Ridge", "Lasso", "ElasticNet",
    "DFM", "DFM2", "XGBoost", "BVAR", "GARCH", "LSTM",
    "Tree", "RandomForest", "GradientBoosting", "ExtraTrees", "StochasticGB"
  ],
  "features": {
    "target_lags": [1, 3, 6, 12, 24],
    "exog": { "__all__": { "lags": [0, 1, 3, 6] } },
    "sweep": {
      "packs": ["none", "ta_basic", "ta_advanced"],
      "normalize": [
        {"method": "none"},
        {"method": "zscore", "window": 12},
        {"method": "zscore", "window": 24},
        {"method": "minmax", "window": 12},
        {"method": "robust", "window": 12}
      ],
      "max_features": [30, 50, 100, 150],
      "exog_combo_k": [3, 4, 5],
      "exog_combo_limit": [6, 8, 10],
      "exog_combo_names": [
        "FX", "JIBOR", "Money Supply", "Stock Index", "Fed Funds",
        "IPI", "PMI", "FPI", "Motor Sales", "Exports", "Imports", 
        "Loans", "Trade Bal", "Gov Bond", "Fiscal Balance", "Terms of Trade",
        "Crude Oil Price", "Import Price Index", "Disposable Income", 
        "Tax Revenue", "Household Debt", "Imports Capital Goods", 
        "Export Price Index", "Manufacturing Wages YoY", "Unemployment",
        "informal_employment"
      ]
    }
  },
  "model_configs": {
    "RandomForest": {
      "n_estimators": [50, 100, 200, 300, 500],
      "max_depth": [3, 5, 7, 10, 15, null],
      "min_samples_split": [2, 5, 10],
      "min_samples_leaf": [1, 2, 4],
      "max_features": ["sqrt", "log2", 0.3, 0.5, 0.7],
      "bootstrap": [true, false],
      "random_state": [42]
    },
    "XGBoost": {
      "n_estimators": [50, 100, 200, 300, 500],
      "max_depth": [3, 5, 7, 10],
      "learning_rate": [0.01, 0.05, 0.1, 0.2, 0.3],
      "subsample": [0.6, 0.8, 1.0],
      "colsample_bytree": [0.6, 0.8, 1.0],
      "reg_alpha": [0, 0.1, 0.5, 1.0],
      "reg_lambda": [1, 1.5, 2.0],
      "random_state": [42]
    },
    "GradientBoosting": {
      "n_estimators": [50, 100, 200, 300],
      "max_depth": [3, 5, 7, 10],
      "learning_rate": [0.01, 0.05, 0.1, 0.2],
      "subsample": [0.6, 0.8, 1.0],
      "max_features": ["sqrt", "log2", 0.3, 0.5],
      "random_state": [42]
    },
    "ExtraTrees": {
      "n_estimators": [50, 100, 200, 300, 500],
      "max_depth": [3, 5, 7, 10, 15, null],
      "min_samples_split": [2, 5, 10],
      "min_samples_leaf": [1, 2, 4],
      "max_features": ["sqrt", "log2", 0.3, 0.5, 0.7],
      "bootstrap": [true, false],
      "random_state": [42]
    },
    "Tree": {
      "max_depth": [3, 5, 7, 10, 15, null],
      "min_samples_split": [2, 5, 10, 20],
      "min_samples_leaf": [1, 2, 4, 8],
      "max_features": ["sqrt", "log2", 0.3, 0.5, 0.7],
      "random_state": [42]
    },
    "LSTM": {
      "units": [32, 64, 128, 256],
      "dropout": [0.1, 0.2, 0.3, 0.5],
      "recurrent_dropout": [0.1, 0.2, 0.3],
      "batch_size": [16, 32, 64],
      "epochs": [50, 100, 200],
      "learning_rate": [0.001, 0.01, 0.1]
    },
    "ARIMA": {
      "p": [0, 1, 2, 3, 4, 5],
      "d": [0, 1, 2],
      "q": [0, 1, 2, 3, 4, 5],
      "seasonal": [false, true],
      "seasonal_periods": [3, 6, 12, 24]
    },
    "Ridge": {
      "alpha": [0.1, 1.0, 10.0, 100.0, 1000.0],
      "solver": ["auto", "svd", "cholesky", "lsqr", "sparse_cg", "sag", "saga"]
    },
    "Lasso": {
      "alpha": [0.001, 0.01, 0.1, 1.0, 10.0],
      "selection": ["cyclic", "random"]
    },
    "ElasticNet": {
      "alpha": [0.001, 0.01, 0.1, 1.0, 10.0],
      "l1_ratio": [0.1, 0.3, 0.5, 0.7, 0.9],
      "selection": ["cyclic", "random"]
    }
  },
  "output": { "dir": "outputs" }
}

