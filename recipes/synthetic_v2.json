{
  "target_id": "y",
  "frequency": "M",
  "horizons": [1, 2, 3],
  "strategy": "frozen",
  "as_of_date": "2025-08-31",
  "train": { "start": "2010-01-31", "end": "2019-12-31" },
  "test": { "start": "2020-01-31", "end": "2025-07-31" },
  "data": {
    "path": "v2/data/synthetic/synthetic_monthly.csv",
    "date_col": "date",
    "synthetic": {
      "enabled": true,
      "n_exog": 3,
      "mode": "mixed",
      "seasonal": true,
      "break_at": "2018-01-31",
