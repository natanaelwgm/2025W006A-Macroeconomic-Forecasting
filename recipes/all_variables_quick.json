{
  "run_name": "indonesia_quick_test",
  "targets": [
    {"id": "cpi_yoy", "name": "CPI YoY", "horizons": [1, 3]},
    {"id": "gdp_yoy", "name": "GDP YoY", "horizons": [1, 3]},
    {"id": "policy_rate_7drr", "name": "Policy Rate", "horizons": [1, 3]},
    {"id": "usd_idr", "name": "USD/IDR", "horizons": [1, 3]},
    {"id": "jci", "name": "JCI", "horizons": [1, 3]},
    {"id": "money_supply", "name": "Money Supply", "horizons": [1, 3]}
  ],
  "frequency": "M",
  "strategy": "frozen",
  "train": { "start": "2010-01-31", "end": "2019-12-31" },
  "test": { "start": "2020-01-31", "end": "2023-12-31" },
  "data": {
    "path": "data/processed/indonesia_macro_monthly.csv",
    "date_col": "date"
  },
  "models_filter": ["RandomForest", "AR1"],
  "features": {
    "target_lags": [1, 12],
    "exog": { "__all__": { "lags": [0] } },
    "sweep": {
      "packs": ["none"],
      "normalize": [{"method": "none"}],
      "max_features": [30],
      "exog_combo_k": 3,
      "exog_combo_limit": 2
    }
  },
  "output": { "dir": "outputs" }
}