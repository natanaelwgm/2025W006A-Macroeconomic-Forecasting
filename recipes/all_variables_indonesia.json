{
  "run_name": "indonesia_all_variables",
  "targets": [
    {
      "id": "cpi_yoy",
      "name": "CPI Year-over-Year",
      "horizons": [1, 3, 6]
    },
    {
      "id": "gdp_yoy",
      "name": "GDP Year-over-Year",
      "horizons": [1, 3, 6]
    },
    {
      "id": "policy_rate_7drr",
      "name": "Policy Rate (7DRR)",
      "horizons": [1, 3, 6]
    },
    {
      "id": "deposit_rate_1m",
      "name": "1-Month Deposit Rate",
      "horizons": [1, 3, 6]
    },
    {
      "id": "deposit_rate_3m",
      "name": "3-Month Deposit Rate",
      "horizons": [1, 3, 6]
    },
    {
      "id": "deposit_rate_6m",
      "name": "6-Month Deposit Rate",
      "horizons": [1, 3, 6]
    },
    {
      "id": "deposit_rate_12m",
      "name": "12-Month Deposit Rate",
      "horizons": [1, 3, 6]
    },
    {
      "id": "usd_idr",
      "name": "USD/IDR Exchange Rate",
      "horizons": [1, 3, 6]
    },
    {
      "id": "yield_10y",
      "name": "10-Year Bond Yield",
      "horizons": [1, 3, 6]
    },
    {
      "id": "jibor_avg",
      "name": "JIBOR Average",
      "horizons": [1, 3, 6]
    },
    {
      "id": "jci",
      "name": "Jakarta Composite Index",
      "horizons": [1, 3, 6]
    },
    {
      "id": "money_supply",
      "name": "Money Supply",
      "horizons": [1, 3, 6]
    }
  ],
  "frequency": "M",
  "strategy": "frozen",
  "train": { "start": "2005-01-31", "end": "2019-12-31" },
  "test": { "start": "2020-01-31", "end": "2025-07-31" },
  "data": {
    "path": "data/processed/indonesia_macro_monthly.csv",
    "date_col": "date"
  },
  "models_filter": [
    "Tree",
    "RandomForest",
    "GradientBoosting",
    "AR1"
  ],
  "features": {
    "target_lags": [1, 3, 12],
    "exog": { "__all__": { "lags": [0, 1] } },
    "sweep": {
      "packs": ["none", "ta_basic"],
      "normalize": [
        {"method": "none"},
        {"method": "zscore", "window": 12}
      ],
      "max_features": [30, 60],
      "exog_combo_k": 3,
      "exog_combo_limit": 3
    }
  },
  "output": { "dir": "outputs" }
}